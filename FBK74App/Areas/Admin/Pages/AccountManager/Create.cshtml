@page
@model FBK74App.Areas.Admin.Pages.AccountManager.CreateModel

@{
    ViewData["Title"] = "Create";
}
<div class="container pt-4">
<h1>Create</h1>

<h4>Account</h4>
<hr />

<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Account.UserName" class="control-label"></label>
                <input asp-for="Account.UserName" class="form-control" />
                <span asp-validation-for="Account.UserName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Account.Password" class="control-label"></label>
                <input asp-for="Account.Password" class="form-control" />
                <span asp-validation-for="Account.Password" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Account.Email" class="control-label"></label>
                <input asp-for="Account.Email" class="form-control" />
                <span asp-validation-for="Account.Email" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Account.PhoneNumber" class="control-label"></label>
                <input asp-for="Account.PhoneNumber" class="form-control" />
                <span asp-validation-for="Account.PhoneNumber" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Account.Type" class="control-label"></label>
                <select asp-for="Account.Type" class="form-control"></select>
                <span asp-validation-for="Account.Type" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Account.Status" class="control-label"></label>
                <input asp-for="Account.Status" class="form-control" />
                <span asp-validation-for="Account.Status" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Account.CreatedDate" class="control-label"></label>
                <input asp-for="Account.CreatedDate" class="form-control" />
                <span asp-validation-for="Account.CreatedDate" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Account.UpdatedDate" class="control-label"></label>
                <input asp-for="Account.UpdatedDate" class="form-control" />
                <span asp-validation-for="Account.UpdatedDate" class="text-danger"></span>
            </div>
            <div class="form-group pt-2 d-flex">
                    <input type="submit" value="Create" class="btn btn-primary mr-2" />
            </div>
        </form>
    </div>
</div>
    <a class="pt-2 ml-2" asp-page="Index">Return</a>
<div >
    
</div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
